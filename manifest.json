{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "2.1",
    "action":
    {
        "default_popup": "popup/popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "description": "Automatically mutes YouTube ads and restores audio for videos.",
    "permissions": ["activeTab", "scripting", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["content.js"]
        }
    ]


}